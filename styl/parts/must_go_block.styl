#must_go_block
	padding: 60px 0 0;
	h4
		text-align center
		font-size 34px
		margin-bottom 20px
	.row
		width 100%

#must_go_bullet
	margin-top 80px
.must_go_ul
	li
		display table
		font-size 34px
		color white
		line-height: 20px;
		float left
		margin-bottom: 35px;
		width: 33.3333%;
		padding: 0 35px;
		overflow hidden
		&:nth-child(2)
			position relative
			top -50px
		img
			margin-right 25px
			float left
			display: table-cell;
			vertical-align: middle;
		span
			font-size 20px
			color #623570
			display: table-cell;
			vertical-align: middle;